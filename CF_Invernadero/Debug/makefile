# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: CF_Invernadero.axf

# Tool invocations
CF_Invernadero.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU Linker'
	arm-none-eabi-gcc -nostdlib -Xlinker -Map="CF_Invernadero.map" -Xlinker --cref -Xlinker --gc-sections -Xlinker -print-memory-usage -mcpu=cortex-m3 -mthumb -T "CF_Invernadero_Debug.ld" -o "CF_Invernadero.axf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(EXECUTABLES)$(OBJS)$(C_DEPS) CF_Invernadero.axf
	-@echo ' '

post-build:
	-@echo 'Performing post-build steps'
	-arm-none-eabi-size "CF_Invernadero.axf"; # arm-none-eabi-objcopy -v -O binary "CF_Invernadero.axf" "CF_Invernadero.bin" ; # checksum -p LPC1769 -d "CF_Invernadero.bin";
	-@echo ' '

.PHONY: all clean dependents post-build

